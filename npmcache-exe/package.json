  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "local": "ng build --environment=dev --target=development --output-path=C:/Sites/bridgesUI/",
    "pipeline-test": "ng test --watch=false --browsers=PhantomJS --reporters=teamcity",
    "pipeline-ci": "ng build --prod --aot=false --environment=ci --output-path=dist/ci",
    "pipeline-devtest": "ng build --prod --aot=false --environment=devtest --output-path=dist/devtest",
    "pipeline-staging": "ng build --prod --aot=false --environment=staging --output-path=dist/staging"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.0",
